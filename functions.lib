# Prints directory path where script is being run from in format like: /home/user/name
function DIRECTORY {
        dirname $0
           }

function HOSTNAME {
        hostname
           }

function LANIP {
        hostname -I
           }

function WANIP {
           host myip.opendns.com resolver1.opendns.com | grep "myip.opendns.com has" | awk '{print $4}'
           }
